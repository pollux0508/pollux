				<artifactId>maven-source-plugin</artifactId>
				<configuration>
					<attach>true</attach>
				</configuration>
				<executions>
					<execution>
						<phase>package</phase>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<configuration>
					<author>true</author>
					<charset>UTF-8</charset>
					<encoding>UTF-8</encoding>
					<docencoding>UTF-8</docencoding>
					<nohelp>true</nohelp>
					<show>protected</show>
				</configuration>
				<executions>
					<execution>
						<id>attach-javadocs</id>
						<phase>deploy</phase>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.sonarsource.scanner.maven</groupId>
				<artifactId>sonar-maven-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.jacoco</groupId>
				<artifactId>jacoco-maven-plugin</artifactId>
				<executions>
					<execution>
						<id>pre-test</id>
						<phase>test-compile</phase>
						<goals>
							<goal>prepare-agent</goal>
						</goals>
					</execution>
					<execution>
						<id>report</id>
						<phase>prepare-package</phase>
						<goals>
							<goal>report</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-archetype-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-assembly-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>versions-maven-plugin</artifactId>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-enforcer-plugin</artifactId>
				<executions>
					<execution>
						<id>enforce</id>
						<phase>validate</phase>
						<goals>
							<goal>enforce</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<rules>
						<bannedDependencies>
							<excludes>
								<exclude>org.springframework:*</exclude>
								<exclude>org.mybatis:*</exclude>
								<exclude>com.fasterxml.jackson:*</exclude>
								<exclude>org.slf4j:*</exclude>
								<exclude>junit:junit</exclude>
								<exclude>org.aspectj:*</exclude>
								<exclude>cglib:*</exclude>
								<exclude>mysql:*</exclude>
								<exclude>javax.servlet:*</exclude>
								<exclude>org.apache.poi:*</exclude>
								<exclude>commons-beanutils:*</exclude>
								<exclude>commons-collections:*</exclude>
								<exclude>commons-codec:*</exclude>
								<exclude>org.apache.commons:*</exclude>
								<exclude>commons-digester:*</exclude>
								<exclude>commons-net:*</exclude>
								<exclude>commons-io:*</exclude>
								<exculde>commons-fileupload:*</exculde>
								<exculde>>commons-logging:*</exculde>
								<exculde>com.atomikos:*</exculde>
								<exculde>javax.transaction:*</exculde>
								<exclude>org.thymeleaf:*</exclude>
								<exclude>com.google.guava:*</exclude>
								<exclude>ch.qos.logback:*</exclude>
								<exclude>com.ckfinder:*</exclude>
								<exclude>org.activiti:*</exclude>
								<exclude>com.github.miemiedev:*</exclude>
								<exclude>jaxen:*</exclude>
							</excludes>
							<includes>
								<include>org.springframework:*:${spring.version}</include>
								<include>org.mybatis:mybatis:${mybatis.version}</include>
								<include>org.mybatis:mybatis-spring:${mybatis-spring.version}</include>
								<include>com.fasterxml.jackson:*:${jackson.version}</include>
								<include>org.slf4j:*:${slf4j.version}</include>
								<include>junit:junit:${junit.version}</include>
								<include>org.aspectj:aspectjrt:${aspectj.version}</include>
								<include>org.aspectj:aspectjweaver:${aspectj.version}</include>
								<include>cglib:*:${cglib.version}</include>
								<include>mysql:mysql-connector-java:${mysql.driver.version}</include>
								<include>javax.servlet:servlet-api:${servlet.version}</include>
								<include>javax.servlet:jstl:${jstl.version}</include>
								<include>org.apache.poi:*:${poi.version}</include>
								<include>commons-beanutils:commons-beanutils:${commons-beanutils.version}</include>
								<include>commons-collections:commons-collections:${commons-collections.version}</include>
								<include>commons-codec:commons-codec:${commons-codec.version}</include>
								<include>org.apache.commons:commons-dbcp2:${commons-dbcp2.version}</include>
								<include>org.apache.commons:commons-pool2:${commons-pool2.version}</include>
								<include>org.apache.commons:commons-lang3:${commons-lang3.version}</include>
								<include>org.apache.commons:commons-text:${commons-text.version}</include>
								<include>org.apache.commons:commons-email:${commons-email.version}</include>
								<include>commons-digester:commons-digester3:${commons-digester3.version}</include>
								<include>commons-net:commons-net:${commons-net.version}</include>
								<include>commons-io:commons-io:${commons-io.version}</include>
								<include>commons-fileupload:commons-fileupload:${commons-fileupload.version}</include>
								<include>commons-logging:commons-logging:${commons-logging.version}</include>
								<include>com.atomikos:*:${atomikos.version}</include>
								<include>javax.transaction:jta:${jta.version}</include>
								<include>org.thymeleaf:thymeleaf:${thymeleaf.version}</include>
								<include>org.thymeleaf:thymeleaf-spring4:${thymeleaf.version}</include>
								<include>com.google.guava:guava:${guava.version}</include>
								<include>com.ckfinder:*:${ckfinder.version}</include>
								<include>org.activiti:*:${activiti.version}</include>
								<include>com.github.miemiedev:mybatis-paginator:${mybatis-paginator.version}</include>
								<include>jaxen:*:${jaxen.version}</include>
								<include>ch.qos.logback:*:${logback.version}</include>
							</includes>
							<searchTransitive>true</searchTransitive>
						</bannedDependencies>
						<requireMavenVersion>
							<version>3.5.0</version>
						</requireMavenVersion>
						<requireJavaVersion>
							<version>1.8</version>
						</requireJavaVersion>
					</rules>
					<fail>true</fail>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<distributionManagement>
		<repository>
			<id>Releases</id>
			<name>Nexus Release</name>
