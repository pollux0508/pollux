		</repository>
		<snapshotRepository>
			<id>Snapshots</id>
			<name>Nexus Snapshot</name>
			<url>http://nexushost:8081/repository/maven-snapshots/</url>
		</snapshotRepository>
	</distributionManagement>
</project>